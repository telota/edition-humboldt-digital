               <choice>
                  <value>TeX</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">TeX-Notation</a:documentation>
                  <value>MathML</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">MathML-Notation</a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="figure">
      <element name="figure">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">groups elements representing or containing graphic information such as an illustration, formula, or figure. [14.4. Specific Elements for Graphic Images]</a:documentation>
         <zeroOrMore>
            <choice>
               <ref name="model.headLike"/>
               <ref name="model.common"/>
               <ref name="figDesc"/>
               <ref name="model.graphicLike"/>
               <ref name="model.global"/>
               <ref name="model.divBottom"/>
            </choice>
         </zeroOrMore>
         <ref name="att.global.attribute.xmlid"/>
