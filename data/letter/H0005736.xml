    <text><body>
            <div type="writingSession" n="1">
                <pb n="1r"/>
                <p>Darf ich Sie, mein Lieber, <lb/>wohl einmal wieder um <lb/>einige Federn<supplied cert="high">,</supplied> 6–8<supplied cert="high">,</supplied>
                        bitten<supplied cert="high">.</supplied>
                    <lb/>Sie halten lange vor<supplied cert="high">.</supplied>
                </p>
                <signed rend="paragraph">Humboldt</signed>
                <dateline rend="align(right)">Montag.</dateline>
                <pb n="1v"/>
                <postscript>
                    <p>
                        <address>
                            <addrLine><choice>
                                    <abbr>Sr</abbr>
                                    <expan>Seiner</expan>
                                </choice> Wohlgeboren</addrLine>
                            <addrLine>Herrn <choice>
                                    <abbr>Prof.</abbr>
                                    <expan>Professor</expan>
                                </choice> Kunth</addrLine>
                        </address>
                    </p>
                </postscript>
            </div>
        </body></text>
